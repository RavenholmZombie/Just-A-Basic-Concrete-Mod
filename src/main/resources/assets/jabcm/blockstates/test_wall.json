{
    "multipart": [
        {
            "when": { "post": "true" },
            "apply": { "model": "jabcm:wall_post_test" }
        },
        {
            "when": { "post_top": "true" },
            "apply": { "model": "jabcm:wall_post_top_test" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "jabcm:wall_post_side_test", "y": 90 }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "jabcm:wall_post_side_test", "y": 180 }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "jabcm:wall_post_side_test", "y": 270 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "jabcm:wall_post_side_test", "y": 0 }
        }
    ]
}